
@model Student
<h1 class="alert alert-success">Detail of @Model.Name </h1>
<div class="alert alert-success ">
    <div class="alert alert-info">
        <h1> <span class="glyphicon glyphicon-lock"></span><b>ID:</b>   @Model.Id</h1>
        <p><span class="glyphicon glyphicon-user"></span><b>Name:</b>   @Model.Name</p>
        <p><span class="glyphicon glyphicon-user"></span><b>FatherName:</b>   @Model.FatherName</p>
        <p><span class="glyphicon glyphicon-bookmark"></span><b>Class:</b>   @Model.Class</p>
        <p><span class="glyphicon glyphicon-book"></span><b>Department:</b>   @Model.Department</p>
        <p><span class="glyphicon glyphicon-book"></span><b>Email:</b>   @Model.Email</p>
</div>
    </div>
<a href="/student/ViewList"><span class="glyphicon glyphicon--arrow-left"></span></a>

